      "case_name": "异常用例-错误密码",
      "params": {
        "userName": "mc123",
        "password": "123123"
      },
      "expected": {
        "resultCode": "-1",
        "resultMsg": "账号或密码不正确"
      }
    },
    {
      "case_name": "异常用例-密码不能为空",
      "params": {
        "userName": "mc123",
        "password": ""
      },
      "expected": {
        "resultCode": "-1",
        "resultMsg": "密码不能为空"
      }
    },
    {
      "case_name": "异常用例-手机号不能为空",
      "params": {
        "userName": "mc123",
        "mobile": ""
      },
      "expected": {
        "resultCode": "-1",
        "resultMsg": "手机号不能为空"
      }
    },
    {
      "case_name": "异常用例-短信内容不能为空",
      "params": {
        "userName": "mc123",
        "content": ""
      },
      "expected": {
        "resultCode": "-1",
        "resultMsg": "短信内容不能为空"
      }
    },
    {
      "case_name": "正向用例-短信内容499字",
      "params": {
        "userName": "mc123",
        "content": ""
      },
      "expected": {
