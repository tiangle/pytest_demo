{
  "dfrc_test_data": [
    {
      "case_name": "正向用例-基本参数发送",
      "params": {
        "userName": "mc123"
      },
      "expected": {
        "resultCode": "1",
        "resultMsg": "提交成功"
      }
    },
    {
      "case_name": "正向用例-自定义seqid",
      "params": {
        "userName": "mc123",
        "seqid": "test_seq_001"
      },
      "expected": {
        "resultCode": "1",
        "resultMsg": "提交成功"
      }
    },
    {
      "case_name": "正向用例-定时发送",
      "params": {
        "userName": "mc123",
        "dstime": "20301231235959"
      },
      "expected": {
        "resultCode": "1",
        "resultMsg": "提交成功"
      }
    },
    {
      "case_name": "正向用例-国际短信",
      "params": {
        "userName": "mc123",
        "isms": 1
      },
      "expected": {
        "resultCode": "1",
        "resultMsg": "提交成功"
      }
    },
    {
      "case_name": "正向用例-多个手机号",
      "params": {
        "userName": "mc123",
