      },
      "expected": {
        "resultCode": "-14",
        "resultMsg": "内容解析异常"
      }
    },
    {
      "case_name": "异常用例-isms值非法",
      "params": {
        "userName": "mc123",
        "isms": 2
      },
      "expected": {
        "resultCode": "-14",
        "resultMsg": "内容解析异常"
      }
    },
    {
      "case_name": "异常用例-错误账号",
      "params": {
        "userName": "mc123456"
      },
